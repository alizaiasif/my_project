<p id="notice"><%= notice %></p>

<h1>Listing Reservattions</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Patient</th>
      <th>Room</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reservattions.each do |reservattion| %>
      <tr>
        <td><%= reservattion.date %></td>
        <td><%= reservattion.patient %></td>
        <td><%= reservattion.room %></td>
        <td><%= link_to 'Show', reservattion %></td>
        <td><%= link_to 'Edit', edit_reservattion_path(reservattion) %></td>
        <td><%= link_to 'Destroy', reservattion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Reservattion', new_reservattion_path %>
